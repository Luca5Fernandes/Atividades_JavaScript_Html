<meta charset="UTF-8">

<script>
	
	function pulaLinha() {

		document.write("<br><br>");		
	}

	function vizualizar(frase) {

		document.write(frase);
		pulaLinha();
	}
	
	for(var multiplicador = 1; multiplicador <= 10; multiplicador++) {

		vizualizar(7 * multiplicador);
	}

	vizualizar("Fim");  
		
</script>




